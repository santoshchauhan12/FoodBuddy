        <style name="StyleButton">
            <item name="android:textColor">@color/white</item>
            <item name="android:textAllCaps">false</item>
            <item name="android:textSize">@dimen/dimen_txt_18</item>
        </style>

        <style name="EditTextStyle">
            <item name="android:textSize">@dimen/dimen_txt_20</item>
            <item name="android:textColor">@color/white</item>
            <item name="android:textColorHint">@color/edit_text_hint_color</item>
        </style>

        <style name="AccentColorTheme" parent="AppTheme.NoActionBar">
            <item name="colorAccent">@color/edit_text_hint_color</item>
        </style>

        <style name="TextAppearance" parent="@android:style/TextAppearance">
            <item name="android:textColor">@color/white</item>

        </style>

        <style name="FbSignupLabelTheme" parent="TextAppearance.AppCompat.Large.Inverse">
            <item name="android:textColorHintInverse">@color/white</item>
            <item name="android:textColorHint">@color/white</item>
            <item name="colorAccent">@color/white</item>
            <item name="colorControlNormal">@color/white</item>
            <item name="colorControlActivated">@color/white</item>
            <item name="android:textSize">15sp</item>

        </style>

        <style name="AWBTextInputLayout" parent="@android:style/TextAppearance">
            <item name="android:textColor">@color/white</item>
            <item name="android:textSize">12sp</item>
        </style>

        <style name="AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

        <style name="PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

        <style name="AppTheme.TabLayout.Title" parent="@android:style/TextAppearance.Widget.TextView">
            <item name="android:textColor">@android:color/white</item>
            <item name="android:textSize">@dimen/dimen_txt_10</item>
            <item name="android:layout_gravity">center</item>
            <item name="android:maxLines">1</item>
            <item name="android:gravity">center</item>
        </style>

        <style name="AccountHeaderStyle">
            <item name="android:textSize">@dimen/dimen_txt_16</item>
            <item name="android:textColor">@color/header_color</item>
        </style>

        <style name="AccountPaymentHeaderStyle">
            <item name="android:textSize">@dimen/dimen_txt_10</item>
            <item name="android:textColor">@color/header_color</item>
            <item name="android:background">@color/white</item>
            <item name="android:layout_marginLeft">@dimen/dimen_16</item>
        </style>

        <style name="AccountPaymentTextStyle">
            <item name="android:textSize">@dimen/dimen_txt_18</item>
            <item name="android:textColor">@color/edittext_color</item>
            <item name="android:textColorHint">@color/edittext_color</item>
            <item name="android:layout_marginLeft">@dimen/dimen_16</item>
            <item name="android:layout_marginRight">@dimen/dimen_16</item>
        </style>

        <style name="styleNotifocationText" parent="Widget.AppCompat.CompoundButton.Switch">
            <item name="android:textSize">@dimen/dimen_txt_16</item>
            <item name="android:textColor">@color/header_color</item>
            <item name="android:paddingLeft">@dimen/dimen_16</item>
            <item name="android:paddingRight">@dimen/dimen_24</item>
            <!--<item name="android:fontFamily">@string/proxima_novasoft_regular</item>-->
        </style>

